#include<iostream>
using namespace std;


int main()
{
	int marks;
	
	cout<<"enter the marks:"<<endl;
	cin>>marks;
	
	if(marks<=25){
		cout<<"F grade"<<endl;
	}if(marks>25 && marks<=45){
		cout<<"E grade"<<endl;
	}if(marks>45 && marks<=50){
		cout<<"D grade"<<endl;
	}if(marks>50 && marks<=60){
		cout<<"C grade"<<endl;
	}if(marks>60 && marks<=70){
		cout<<"B grade"<<endl;
	}if(marks>70 && marks<=80){
		cout<<"A grade"<<endl;
	}if(marks>80){
		cout<<"O grade"<<endl;
	}
}
